{
  "name": "save-it-v2",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3",
    "yarn": ">=1.15.2"
  },
  "scripts": {
    "build": "yarn --cwd app install && yarn --cwd app build",
    "api:test": "(for /F \"tokens=*\" %i in (api/.env) do set %i) && cargo test --manifest-path api/Cargo.toml",
    "api:start": "(for /F \"tokens=*\" %i in (api/.env) do set %i) &&cargo run --manifest-path api/Cargo.toml",
    "app:start": "yarn --cwd app start",
    "app:start-local-api": "yarn --cwd app start-local-api",
    "app:test": "yarn --cwd app test --watchAll=false",
    "api:start-e2e": "cargo run --manifest-path api/Cargo.toml",
    "app:start-e2e": "yarn --cwd app install && yarn --cwd app start-local-api",
    "start:all": "npm-run-all --parallel api:start-e2e app:start-e2e",
    "cy:run": "yarn --cwd e2e install && yarn --cwd e2e e2e",
    "e2e": "start-test api:start-e2e http-get://localhost:8080/api/v1/healthcheck app:start-e2e 3000 cy:run",
    "poste2e": "kill-port 3000"
  },
  "dependencies": {},
  "devDependencies": {
    "kill-port": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.12.6"
  }
}
